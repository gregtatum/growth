{
  "name": "growth",
  "version": "0.0.1",
  "browserify": {
    "transform": [
      "glslify",
      "brfs"
    ]
  },
  "scripts": {
    "build": "browserify ./js/index.js -d -g [uglifyify -x .js] | exorcist assets/build/bundle.js.map > assets/build/bundle.js",
    "start": "budo js/index.js:assets/build/bundle.js --live --verbose | garnish"
  },
  "devDependencies": {
    "beefy": "^2.1.3",
    "brfs": "^1.4.0",
    "budo": "^4.0.0",
    "crossroads": "^0.12.0",
    "events": "^1.0.2",
    "exorcist": "^0.4.0",
    "glsl-hsv2rgb": "^1.0.0",
    "glsl-noise": "0.0.0",
    "glsl-random": "0.0.4",
    "glslify": "^1.6.0",
    "hasher": "^1.2.0",
    "insert-css": "^0.2.0",
    "localforage": "^1.2.0",
    "perlin-simplex": "0.0.2",
    "pex-geom": "^0.14.0",
    "rsvp": "^3.0.14",
    "soundcloud-badge": "0.0.0",
    "three-glslify": "^2.0.1",
    "uglifyify": "^3.0.1",
    "watchify": "^2.4.0"
  },
  "dependencies": {
    "underscore": "^1.7.0"
  }
}
